{
  "workbench.colorTheme": "Monokai",
  "workbench.colorCustomizations": {
    "editorError.foreground": "#FF0000",
    "editorError.border": "#FF0000",
    "editorWarning.foreground": "#ffd51d", // https://stackoverflow.com/questions/40697223/how-to-change-error-styles-in-vs-code
    "editorWarning.border": "#ffd51d",
    "editorWhitespace.foreground": "#A5A1AE",
    "editor.selectionBackground": "#ff3d02",
    "editor.wordHighlightBackground": "#b96750",
    "editor.findMatchBackground": "#0228ff",
    "editor.wordHighlightStrongBackground": "#a76750",
    "editorRuler.foreground": "#ff333388"
  },
  "editor.fontFamily": "'Ubuntu Mono', 'Droid Sans Mono', 'Monaco', 'Courier New', monospace, 'Droid Sans Fallback'",
  "editor.renderWhitespace": "boundary",
  // "workbench.iconTheme": "vscode-great-icons",
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.alwaysShowStatus": true,
  // "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.printWidth": 140,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[md]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "files.exclude": {
    "**/dist/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    ".tmpdir*": true,
    ".tmpdir": true,
    "**/node_modules/**": true,
    "**/.git": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.svn": true,
    "build/**": true,
    ".build/**": true,
    "**/obj/**": true,
    "**/cooked/node_modules": false,
    "**/.build": true,
    "migbase/include/cmdbapi/cmdbapi.h": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    ".tmpdir": true,
    ".tmpdir*": true,
    "**/node_modules/**": true,
    "**/.git": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.svn": true,
    "build/**": true,
    ".build/**": true,
    "**/obj/**": true,
    "**/cooked/node_modules": true,
    "**/.build": true
  },
  "editor.rulers": [100, 120],
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.minimap.enabled": false,
  "editor.autoIndent": "full",
  "search.exclude": {
    "**/node_modules": true,
    "**/cooked/node_modules": true,
    "**/.angular/cache": true,
    "**/.build": true,
    ".tmpdir*": true,
    "**/node_modules/**": true,
    "**/.git": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.svn": true,
    "build/**": true,
    ".build/**": true,
    "**/obj/**": true
  },
  "C_Cpp.intelliSenseEngine": "Default",
  "C_Cpp.intelliSenseEngineFallback": "Disabled",
  "C_Cpp.default.includePath": [
    "${workspaceRoot}/fortiweb/include",
    "${workspaceRoot}/fortiweb/server",
    "${workspaceRoot}/fortiweb/modules",
    "${workspaceRoot}/migbase/sysapi",
    "${workspaceRoot}/migbase/include/migapi",
    "${workspaceRoot}/scanunit/cfg",
    "${workspaceRoot}/scanunit",
    "${workspaceRoot}/scanunit/include",
    "${workspaceRoot}/fortiweb/modules/api",
    "${workspaceRoot}/migbase/sysapi/jemalloc/include",
    "${workspaceRoot}",
    "${workspaceRoot}/cmf/include",
    "${workspaceRoot}/migbase/include",
    "${workspaceRoot}/fgtcommon",
    "${workspaceRoot}/fgtutil",
    "${workspaceRoot}/include",
    "${workspaceRoot}/kernel/include",
    "${workspaceRoot}/migbase/include/openldap",
    "${workspaceRoot}/migbase/openssl/include",
    "${workspaceRoot}/migbase/openssl/fn_ext",
    "${workspaceRoot}/fortipkg/include/x86_64",
    "${workspaceRoot}/fortipkg/include",
    "/fortidev4/include/linux",
    "/fortidev4/include/c++/4.7.1/tr1",
    "/fortidev4/include/c++/4.7.1/i686-pc-linux-gnu/",
    "/usr/include/c++/6.3.1",
    "/usr/local/include",
    "/usr/lib/clang/4.0.0/include",
    "/usr/include"
  ],
  "C_Cpp.default.defines": [
    "BIG_SECURITY_HOLE",
    "APACHE_SSL",
    "LINUX=22",
    "TARGET=\"init\"",
    "USE_HSREGEX",
    "NO_DL_NEEDED"
  ],
  "C_Cpp.default.browse.databaseFilename": "",
  "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": true,
  "C_Cpp.default.browse.path": [
    "${workspaceRoot}",
    "/usr/include/c++/6.3.1",
    "/usr/local/include",
    "/usr/lib/clang/4.0.0/include",
    "/usr/include"
  ],
  "C_Cpp.default.intelliSenseMode": "clang-x64",
  "C_Cpp.default.compilerPath": "/usr/bin/clang",
  "C_Cpp.default.cStandard": "c11",
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.workspaceParsingPriority": "low",
  "git.autoRepositoryDetection": false,
  "gitlens.keymap": "alternate",
  "global-config.folder": "~/.vscode-insiders/",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.historyExplorer.enabled": true,
  "window.menuBarVisibility": "toggle",
  "terminal.integrated.rendererType": "dom",
  "editor.tabCompletion": "on",
  "editor.fontSize": 13,
  "gitlens.hovers.annotations.changes": false,
  "editor.scrollbar.verticalScrollbarSize": 7,
  "workbench.editor.highlightModifiedTabs": true,
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "zenMode.restore": true,
  "zenMode.fullScreen": false,
  "window.titleBarStyle": "custom",
  "window.nativeTabs": false,
  "workbench.panel.defaultLocation": "right",
  "pasteAndIndent.selectAfter": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "tabnine.experimentalAutoImports": true,
  "workbench.editor.enablePreview": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "problems.showCurrentInStatus": true,
  "security.workspace.trust.untrustedFiles": "open",
  "vim.surround": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["l", "l"],
      "after": ["<Esc>"]
    },
    {
      "before": ["e", "e"],
      "after": ["<Esc>"]
    },
    {
      "before": ["k", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["w", "w"],
      "after": ["<Esc>"]
    },
    {
      "before": ["b", "b"],
      "after": ["<Esc>"]
    },
    {
      "before": ["h", "h"],
      "after": ["<Esc>"]
    },
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["K"],
      "commands": ["lineBreakInsert"],
      "silent": true
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
  },
  "vim.neovimPath": "/home/kguo/app/nvim.appimage",
  "vim.neovimConfigPath": "/home/kguo/.vimrc",
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "editor.stickyScroll.enabled": true,
  "editor.lineNumbers": "relative",
  "editor.glyphMargin": true,
  "editor.accessibilitySupport": "off"
}
