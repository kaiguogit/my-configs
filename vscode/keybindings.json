// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+shift+n",
        "command": "explorer.newFile"
    },
    {
        "key": "ctrl+4",
        "command": "workbench.action.focusPanel"
    },
    {
        "key": "ctrl+shift+d",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+down",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+`",
        "command": "workbench.action.toggleMaximizedPanel"
    },
    {
        "key": "ctrl+shift+t",
        "command": "-taskmark.toggleMark",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+r",
        "command": "-taskmark.previousMark",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+z",
        "command": "-taskmark.nextMark",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+k d",
        "command": "-workbench.files.action.compareWithSaved"
    },
    {
        "key": "ctrl+shift+d",
        "command": "-workbench.view.debug"
    },
    {
        "key": "ctrl+d",
        "command": "editor.action.addSelectionToNextFindMatch"
    },
    {
        "key": "ctrl+alt+d",
        "command": "editor.action.moveSelectionToNextFindMatch",
        "when": "editorFocus"
    },
    {
        "key": "alt+d",
        "command": "editor.action.addSelectionToPreviousFindMatch"
    },
    {
        "key": "ctrl+k ctrl+d",
        "command": "-editor.action.moveSelectionToNextFindMatch",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+e",
        "command": "workbench.view.explorer"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-workbench.view.explorer"
    },
    {
        "key": "ctrl+shift+e",
        "command": "workbench.action.quickOpenNavigatePreviousInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "cmd+v",
        "command": "pasteAndIndent.action",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+v",
        "command": "editor.action.clipboardPasteAction",
        "when": "!editorTextFocus"
    },
    {
        "key": "cmd+shift+v",
        "command": "editor.action.clipboardPasteAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+pagedown",
        "command": "workbench.action.moveEditorToNextGroup"
    },
    {
        "key": "ctrl+alt+right",
        "command": "-workbench.action.moveEditorToNextGroup"
    },
    {
        "key": "ctrl+shift+alt+pageup",
        "command": "workbench.action.moveEditorToPreviousGroup"
    },
    {
        "key": "ctrl+alt+left",
        "command": "-workbench.action.moveEditorToPreviousGroup"
    },
    {
        "key": "ctrl+meta+right",
        "command": "workbench.action.increaseViewSize"
    },
    {
        "key": "ctrl+meta+left",
        "command": "workbench.action.decreaseViewSize"
    },
    {
        "key": "alt+u",
        "command": "cursorRedo",
        "when": "textInputFocus"
    },
    {
        "key": "f2",
        "command": "workbench.action.showCommands"
    },
    {
        "key": "f1",
        "command": "-workbench.action.showCommands"
    },
    {
        "command":"vscode-neovim.escape",
        "key": "`",
        "when": "neovim.mode == insert && editorTextFocus",
    },
    {
        "command": "vscode-neovim.compositeEscape1",
        "key": "w",
        "when": "neovim.mode == insert && editorTextFocus",
        "args": "w"
    },
    {
        "command": "vscode-neovim.compositeEscape1",
        "key": "b",
        "when": "neovim.mode == insert && editorTextFocus",
        "args": "b"
    },
    {
        "command": "vscode-neovim.compositeEscape1",
        "key": "j",
        "when": "neovim.mode == insert && editorTextFocus",
        "args": "j",
    },
    {
        "command": "vscode-neovim.compositeEscape1",
        "key": "k",
        "when": "neovim.mode == insert && editorTextFocus",
        "args": "k"
    },
    {
        "command": "editor.action.moveLinesDownAction",
        "key": "shift+j",
        "when": "neovim.mode == 'normal' && editorTextFocus",
    },
    {
        "command": "editor.action.moveLinesUpAction",
        "key": "shift+k",
        "when": "neovim.mode == 'normal' && editorTextFocus",
    },
    {
        "key": "ctrl+d",
        "command": "-editor.action.outdentLines",
        "when": "editorTextFocus && neovim.ctrlKeysInsert && !neovim.recording && neovim.mode == 'insert'"
    },
    {
        "key": "ctrl+shift+b",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+]",
        "command": "-vscode-neovim.send",
        "when": "editorTextFocus && neovim.ctrlKeysNormal && neovim.init && neovim.mode != 'insert'"
    },
    {
        "key": "ctrl+[",
        "command": "-vscode-neovim.escape",
        "when": "editorTextFocus && neovim.init"
    },
    {
        "key": "ctrl+n",
        "command": "-workbench.action.files.newUntitledFile"
    },
    {
        "key": "ctrl+shift+.",
        "command": "relative-path-and-line.copyRelativePath"
    },
    {
        "key": "ctrl+o",
        "command": "-workbench.action.files.openLocalFile",
        "when": "remoteFileDialogVisible"
    },
    {
        "key": "ctrl+o",
        "command": "-workbench.action.files.openFile",
        "when": "true"
    },
    {
        "key": "ctrl+o",
        "command": "-workbench.action.files.openFileFolder",
        "when": "isMacNative && openFolderWorkspaceSupport"
    },
    {
        "key": "ctrl+o",
        "command": "-workbench.action.files.openFolderViaWorkspace",
        "when": "!openFolderWorkspaceSupport && workbenchState == 'workspace'"
    },
    {
        "key": "ctrl+o",
        "command": "-vscode-neovim.send",
        "when": "editorTextFocus && neovim.ctrlKeysNormal && neovim.init && neovim.mode != 'insert'"
    },
    {
        "key": "j",
        "command": "selectNextSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && neovim.mode == 'normal' && editorTextFocus"
    },
    {
        "key": "k",
        "command": "selectPrevSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && neovim.mode == 'normal' && editorTextFocus"
    },
    {
        "key": "k",
        "command": "list.focusUp",
        "when": "listFocus && !inputFocus"
    },
    {
        "key": "up",
        "command": "-list.focusUp",
        "when": "listFocus && !inputFocus"
    },
    {
        "key": "k",
        "command": "selectPrevCodeAction",
        "when": "codeActionMenuVisible"
    },
    {
        "key": "j",
        "command": "selectNextCodeAction",
        "when": "codeActionMenuVisible"
    },
]